{
  "openapi": "3.0.1",
  "info": {
    "title": "AuthApi.Presentation",
    "version": "1.0"
  },
  "paths": {
    "/api/User/register": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
